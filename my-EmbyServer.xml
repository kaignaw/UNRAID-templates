<?xml version="1.0"?>
<Container version="2">
  <Name>EmbyServer</Name>
  <Repository>emby_crack:latest</Repository>
  <Registry>https://registry.hub.docker.com/r/emby/embyserver/</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://lime-technology.com/forum/index.php?topic=45444.0</Support>
  <Project/>
  <Overview>&#xD;
    Emby Server is a home media server built on top of other popular open source technologies such as Service Stack, jQuery, jQuery mobile, and Mono. Bringing together movies, music, television and more in one dazzling interface.[br][br]&#xD;
    [b][span style='color: #E80000;']Directories:[/span][/b][br]&#xD;
    [b]/config[/b] : this path is used to store Emby's configuration.[br]&#xD;
    [b]/mnt[/b] : this path is used to add library locations to Emby.[br]&#xD;
  </Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:8096]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/MediaBrowser/Emby.Build/master/unraid-templates/emby/EmbyServer.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/MediaBrowser/Emby.Resources/master/images/Logos/logoicon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1643183363</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>&#xD;
    Emby Server is a home media server built on top of other popular open source technologies such as Service Stack, jQuery, jQuery mobile, and Mono. Bringing together movies, music, television and more in one dazzling interface.[br][br]&#xD;
    [b][span style='color: #E80000;']Directories:[/span][/b][br]&#xD;
    [b]/config[/b] : this path is used to store Emby's configuration.[br]&#xD;
    [b]/mnt[/b] : this path is used to add library locations to Emby.[br]&#xD;
  </Description>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>8096</HostPort>
        <ContainerPort>8096</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt</HostDir>
      <ContainerDir>/mnt</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/EmbyServer</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>1000</Value>
      <Name>UID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>GID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>GIDLIST</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Port 1" Target="8096" Default="8096" Mode="tcp" Description="n/a" Type="Port" Display="always" Required="true" Mask="false">8096</Config>
  <Config Name="Host Path 2" Target="/mnt" Default="/mnt" Mode="rw" Description="Container Path: /mnt" Type="Path" Display="always" Required="true" Mask="false">/mnt</Config>
  <Config Name="Key 1" Target="UID" Default="2" Mode="" Description="Container Variable: UID" Type="Variable" Display="always" Required="false" Mask="false">1000</Config>
  <Config Name="Key 2" Target="GID" Default="2" Mode="" Description="Container Variable: GID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="Key 3" Target="GIDLIST" Default="2" Mode="" Description="Container Variable: GIDLIST" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="Graphic" Target="" Default="" Mode="" Description="&#x5BB9;&#x5668; Device: " Type="Device" Display="always" Required="false" Mask="false">/dev/dri</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/EmbyServer" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/EmbyServer</Config>
</Container>
